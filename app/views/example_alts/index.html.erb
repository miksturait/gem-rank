<h1>Listing example_alts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Example</th>
      <th>Description</th>
      <th>Votes</th>
      <th>Rank</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @example_alts.each do |example_alt| %>
      <tr>
        <td><%= example_alt.user %></td>
        <td><%= example_alt.example %></td>
        <td><%= example_alt.description %></td>
        <td><%= example_alt.votes %></td>
        <td><%= example_alt.rank %></td>
        <td><%= link_to 'Show', example_alt %></td>
        <td><%= link_to 'Edit', edit_example_alt_path(example_alt) %></td>
        <td><%= link_to 'Destroy', example_alt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Example alt', new_example_alt_path %>
